{
  "agent": {
    "play_sound_when_agent_done": true,
    "always_allow_tool_actions": true,
    "default_profile": "write",
    "inline_assistant_model": {
      "provider": "copilot_chat",
      "model": "claude-sonnet-4"
    },
    "default_model": {
      "model": "claude-sonnet-4",
      "provider": "copilot_chat"
    }
  },
  "autosave": "on_focus_change",
  "base_keymap": "VSCode",
  "buffer_font_family": "JetBrainsMono Nerd Font",
  "buffer_font_size": 17.0,
  "buffer_line_height": "comfortable",
  "confirm_quit": true,
  "cursor_blink": false,
  "features": {
    "edit_prediction_provider": "copilot"
  },
  "edit_predictions": {
    "copilot": {
      "proxy": null,
      "proxy_no_verify": null
    },
    "mode": "subtle"
  },
  "format_on_save": "on",
  "formatter": "language_server",
  "hide_mouse": "on_typing_and_movement",
  "icon_theme": "Material Icon Theme",
  "indent_guides": {
    "coloring": "fixed",
    "enabled": true
  },
  "inlay_hints": {
    "edit_debounce_ms": 700,
    "enabled": false,
    "scroll_debounce_ms": 50,
    "show_other_hints": true,
    "show_parameter_hints": true,
    "show_type_hints": true
  },
  "lsp": {
    "gopls": {
      "initialization_options": {
        "gofumpt": true
      }
    },
    "pyright": {
      "settings": {
        "python": {
          "pythonPath": "~/.venv/bin/python"
        }
      }
    },
    "languages": {}
  },
  "languages": {
    "TypeScript": {
      "formatter": {
        "external": {
          "command": "prettier",
          "arguments": [
            "--write",
            "--stdin-filepath"
          ]
        }
      }
    }
  },
  "git_panel": {
    "dock": "right"
  },
  "project_panel": {
    "auto_fold_dirs": true,
    "auto_reveal_entries": true,
    "button": true,
    "default_width": 240,
    "dock": "right",
    "file_icons": true,
    "folder_icons": true,
    "git_status": true,
    "indent_size": 20,
    "scrollbar": {
      "show": "always"
    }
  },
  "show_edit_predictions": true,
  "show_wrap_guides": true,
  "soft_wrap": "editor_width",
  "ssh_connections": [
    {
      "args": [
        "-i",
        "~/WorkSpace/Utils/sshkey-vm/main/ssh-key-2025-03-01.key"
      ],
      "host": "158.180.230.169",
      "projects": [
        {
          "paths": [
            "/etc/nginx"
          ]
        },
        {
          "paths": [
            "~/DiscordCTFHelper"
          ]
        },
        {
          "paths": [
            "~/umami"
          ]
        }
      ],
      "username": "ubuntu"
    }
  ],
  "tabs": {
    "close_position": "right",
    "file_icons": true,
    "git_status": true
  },
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "terminal": {
    "alternate_scroll": "off",
    "blinking": "terminal_controlled",
    "button": false,
    "copy_on_select": false,
    "detect_venv": {
      "on": {
        "directories": [
          ".env",
          "env",
          ".venv",
          "venv"
        ]
      }
    },
    "dock": "bottom",
    "font_family": "JetBrainsMono Nerd Font",
    "line_height": "comfortable",
    "option_as_meta": true,
    "shell": "system",
    "toolbar": {},
    "working_directory": "current_project_directory"
  },
  "theme": {
    "dark": "BetterOneDark (default)",
    "light": "One Light",
    "mode": "system"
  },
  "toolbar": {
    "breadcrumbs": true,
    "quick_actions": false,
    "selections_menu": true
  },
  "ui_font_size": 16,
  "vim_mode": false
}
